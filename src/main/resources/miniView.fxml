<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200.0" prefWidth="202.0">
      <children>
         <VBox alignment="BOTTOM_CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
               <Text fx:id="textArea" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" wrappingWidth="200.0" />
               <HBox alignment="BOTTOM_LEFT" prefHeight="47.0" prefWidth="200.0">
                  <children>
                     <Hyperlink fx:id="hyperLink" />
                  </children>
               </HBox>
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>
